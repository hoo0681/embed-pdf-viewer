{
  "name": "@embedpdf/react-pdf-viewer",
  "version": "2.1.2",
  "description": "React component for embedding PDF documents",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "homepage": "https://www.embedpdf.com/react-pdf-viewer",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "pnpm run clean && vite build"
  },
  "dependencies": {
    "@embedpdf/snippet": "workspace:*"
  },
  "devDependencies": {
    "@types/react": "^19.2.7",
    "@types/react-dom": "^18.2.0",
    "rimraf": "^6.1.2",
    "typescript": "^5.1.6",
    "unplugin-dts": "1.0.0-beta.6",
    "vite": "^6.3.5"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "keywords": [
    "react",
    "pdf",
    "viewer",
    "embed",
    "document"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/embedpdf/embed-pdf-viewer.git",
    "directory": "viewers/react"
  },
  "publishConfig": {
    "access": "public"
  }
}
